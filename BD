--
-- TOC entry 227 (class 1255 OID 16508)
-- Name: registrar_log_compra(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.registrar_log_compra() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
    INSERT INTO LogCompra (compra_id)
    VALUES (NEW.id);
    RETURN NEW;
END;
$$;


ALTER FUNCTION public.registrar_log_compra() OWNER TO postgres;

--
-- TOC entry 228 (class 1255 OID 16499)
-- Name: total_compras_cliente(integer); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION public.total_compras_cliente(cli_id integer) RETURNS integer
    LANGUAGE plpgsql
    AS $$
DECLARE
    total INT;
BEGIN
    SELECT COUNT(*) INTO total
    FROM Compra
    WHERE cliente_id = cli_id;
    RETURN total;
END;
$$;


ALTER FUNCTION public.total_compras_cliente(cli_id integer) OWNER TO postgres;

SET default_tablespace = '';

